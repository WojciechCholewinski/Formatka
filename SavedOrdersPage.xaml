<Page x:Class="Formatka.SavedOrdersPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Formatka"
      mc:Ignorable="d" 
      d:DesignHeight="1000" d:DesignWidth="1300"
      Title="SavedOrdersPage">

    <Grid>
        <Button
            x:Name="backBtn"
            Style="{DynamicResource CircleShapeButtonStyle}"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Width="45"
            Height="45"
            Margin="40,40,0,0"
            Click="backBtn_Click">
            <Image Source="Icons/Back_Icon.png"/>
        </Button>
    </Grid>
    
</Page>
